@InCollection{Checkhik03,
  author =	 {Gal Chechik and Amir Globerson and Naftali Tishby
                  and Yair Weiss},
  title =	 {Information Bottleneck for Gaussian Variables},
  booktitle =	 {NIPS-2003},
  OPTcrossref =	 {},
  OPTkey =	 {},
  OPTpages =	 {},
  OPTpublisher = {},
  OPTyear =	 {},
  OPTeditor =	 {},
  OPTvolume =	 {},
  OPTnumber =	 {},
  OPTseries =	 {},
  OPTtype =	 {},
  OPTchapter =	 {},
  OPTaddress =	 {},
  OPTedition =	 {},
  OPTmonth =	 {},
  note =	 {submitted},
  annote =	 {Builds a representation T=AX+\ksi of Gaussian X
                  by transforming linearly and adding noise. The cost
                  function is I(X;T)-\beta I(T;Y), where Y is Gaussian
                  auxiliary variable. Becomes very close to CCA, the
                  problem can be solved in a closed form, and
                  eigenvectors of the same matrix are involved. The
                  main difference is that the part that compresses X
                  is not scale invariant and hence the result is not
                  symmetric. Some very neat results have been derived
                  on how the result depends on the noise (or actually
                  the compromise governed by \beta). A main question
                  remains: What would be the theoretical justification
                  for adding the noise? Becker's Imax or CCA don't use
                  it.}
}

