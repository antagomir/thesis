@Article{Hofmann01,
  author =	 {T. Hofmann},
  title =	 {Unsupervised Learning by Probabilistic Latent
                  Semantic Analysis},
  journal =	 {Machine Learning},
  year =	 2001,
  volume =	 42,
  pages =	 {177-196},
  annote =	 {The most thorough presentation of PLSA (or SMM),
                  probabilistic factor decomposition of co-occurrence
                  data. Introduces the model, its optimization by EM
                  or Tempered EM with early stopping, interprets PLSA
                  as a factor decomposition of the co-occurrences,
                  compares it to LSA and clustering (exact relation to
                  clustering remains unclear to me). Yields better
                  likelihoods (perplexity) than LSA which is no
                  wonder. TEM is better than EM. In information
                  retrieval (IR) PLSA is better as well; queries are
                  folded-in by fixing some parameters. The similarity
                  measure in IR is a weird combination of cosines in
                  the "latent factor space" and the original cosines
                  of the whole vectors.}
}

